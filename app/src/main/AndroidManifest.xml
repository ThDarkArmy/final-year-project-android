<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tda.finalyear">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.admin.AdminActivity"></activity>
        <activity android:name=".activities.admin.AdminSignupActivity" />
        <activity android:name=".activities.admin.AdminLoginActivity" />
        <activity android:name=".activities.teacher.TeacherActivity" />
        <activity android:name=".activities.teacher.TeacherSignupActivity" />
        <activity android:name=".activities.teacher.TeacherLoginActivity" />
        <activity android:name=".activities.student.StudentActivity" />
        <activity android:name=".activities.student.StudentSignupActivity" />
        <activity android:name=".activities.student.StudentLoginActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>